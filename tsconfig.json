{
	"compilerOptions": {
		"target": "ESNext",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"allowJs": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"allowImportingTsExtensions": true,
		"noUnusedParameters": false,
		"noUnusedLocals": false,
		"noEmit": true,
		"skipLibCheck": true,
		"noImplicitAny": true,
		"types": ["naive-ui/volar"],
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		}
	}
}
